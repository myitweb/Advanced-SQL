28.08.2011

create table emp31
(
eid	number(10) 	primary key,
ename	varchar2(50)		   ,
mgrid	number(10)
);	

alter table emp31 add constraint kooo foreign key (mgrid) references emp31(eid);



insert into emp31 values
(100,'x',null);

insert into emp31 values
(101,'y',100);

insert into emp31 values
(102,'z',101);

insert into emp31 values
(103,'p',102);


select eid,ename,mgrid
from emp31
connect by prior eid=mgrid;

select eid,ename,mgrid
from emp31
connect by eid=prior mgrid;




select eid,ename,mgrid
from emp31
start with eid=100
connect by prior eid=mgrid;
order by level;


select eid,ename,mgrid
from emp31
start with eid=102
connect by prior eid=mgrid;
order by level;

